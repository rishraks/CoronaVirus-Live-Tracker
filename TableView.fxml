<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.TableViewController">
   <children>
      <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="sno" minWidth="-1.0" prefWidth="-1.0" text="Serial No" />
          <TableColumn fx:id="state" minWidth="-1.0" prefWidth="-1.0" text="Name of State/UT" />
            <TableColumn fx:id="activeCases" minWidth="-1.0" prefWidth="-1.0" text="Active Cases" />
            <TableColumn fx:id="cured" minWidth="-1.0" prefWidth="-1.0" text="Cured/Discharged/Migrated" />
            <TableColumn fx:id="death" minWidth="-1.0" prefWidth="-1.0" text="Deaths" />
            <TableColumn fx:id="confirmedCase" minWidth="-1.0" prefWidth="-1.0" text="Total Confirmed Cases" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
